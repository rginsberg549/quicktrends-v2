<header class="row navbar navbar-light bg-light border">
  <div class="col">
    <div id="title" class="text-align-middle">
      <img class="logo img-fluid" src="http://localhost:3000/assets/pictures/logo_transparent.png" alt="">
    </div>
  </div>
  <div class="col d-flex justify-content-end">
    <div class="top-buttons">
      <button type="button" onclick="location.href='/logout'" id="login-button" class="btn">Logout</button>
      <button type="button" onclick="location.href='/'" id="signup-button" class="btn bg-info">Home</button>
    </div>
</header>


<section class="row mx-auto d-flex justify-content-center m-3 mb-5">
  <form class="mx-auto d-flex justify-content-center" action="" method="POST">
    <input id="stockInput" class="" type="text" placeholder="ex. AAPL,MSFT">
    <button id="stockSearch" value="stock-symbol" class="btn btn-secondary">Search</button>
  </form>
  <span id="time-remaining"></span>
</section>

<section id="company-card-section" class="row mx-auto d-flex justify-content-center">
  <div>
  
  <div id="company-card" class="card h-100 col-sm-auto mx-0" style="width: 40rem ">
    <div class="mx-auto py-2 d-flex justify-content-center">
      <img class="company-image mr-3" width="40" height="40">
      <h2 class="company-name">COMPANY NAME</h2>
    </div>
    <ul id="company-card-list" class="mx-5">
      <li id="trend-text" class="h6">Trend Text</li>
      <li class="date h6"><time datetime="2016-04-01T19:00">10-10-2020</time></li>
      <li class="ceo-name h6">CEO-NAME</li>
      <li class="industry h6">INDUSTRY</li>
      <li class="current-price h6">CURRENT PRICE</li>
      <li class="eps h6">EPS</li>
      <li class="dividends h6">DIVIDENDS</li>
      <li class="gross-profit-ratio h6">GROSS PROFIT RATIO</li>
      <li class="net-income-ratio h6">NET INCOME RATIO</li>
    </ul>
    <div id="stock-chart-section" class="row">
    </div>
    <div id="recents-card" class="card h-100 col-sm-auto">
      <h2>Recent Searches</h2>
      <ul id="recents-list" class="list-group list-group-flush">
        {{#each stocks}}
        <li><button>{{this.name}}</button></li>
        {{/each}}
      </ul>
    </div>
  </div>

